{{!-- <h2>Your score is {{score}} out of {{questions.length}}.</h2> --}}
{{#if message}}
   <script>
    if (confirm('{{message}}')) {
      window.history.back();
    }else{
        window.history.back();
    }
  </script>
{{/if}}
{{#if check}}
    <style>
    .my-bar {
        text-align: center;
        width: 100%;
        height: 100px;
        border-radius: 25px;
        margin-top: 10px;
        background-color: rgba(92, 222, 236, 0.5);
        {{!-- border-color: black; --}}
        {{!-- border: 10px; --}}
    }
</style>

<div style="height: auto; margin:100px;">
    <div>{{user.fullName}}</div>
    {{#each scores}}
    <div class="my-bar">
        <div>Số câu đúng: {{this.score}}/{{this.amoutquest}}</div>
        <div>Điểm: {{#with (caculationScore this.score this.amoutquest)}} {{this}} {{/with}}</div>
        <div>Thời gian làm: {{this.timecomplete}}</div>
    </div>

    {{/each}}

</div>
{{/if}}
